{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-vercel-toolbar",
          "value": "none"
        }
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' https://vercel.live; connect-src 'self'; style-src 'self';"
        }
      ]
    }
  ]
  }